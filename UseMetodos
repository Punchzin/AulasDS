//acesso a métodos de outra classe
public class UseMetodos {
	public static void main(String args[]) {
		MetodoSemRetorno.imprimirTexto("Usando métodos de outra classe");
		MetodoSemRetorno.somar(20, 30);
		System.out.println(MetodoComRetorno.mostrarDiaPorExtenso(5));
		System.out.println(MetodoComRetorno.contarLetrasA("Marmelada"));
		
	}
}
